{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES6",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./",
        "rootDir": "./",
        "moduleResolution": "node"
    },
    "include": ["main.ts"],
    "exclude": ["node_modules"]
}
